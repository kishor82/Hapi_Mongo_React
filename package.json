{
  "name": "mongohapireactnode",
  "version": "1.0.0",
  "description": "React app with hapi and mongodb. ",
  "main": "./dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "preinstall": "NPM_CONFIG_PRODUCTION=false && node ./preinstall_check",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "build:prod": "npm run build && rm -rf build && cd client && yarn build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn install --prefix client && yarn build:prod",
    "start": "node ./dist/index.js",
    "dev": "ts-node-dev --files src/index.ts"
  },
  "author": "KishorRathva",
  "license": "MIT",
  "engines": {
    "node": "^12.14.0",
    "yarn": "^1.21.1"
  },
  "dependencies": {
    "@elastic/good": "8.1.1-kibana2",
    "@elastic/numeral": "^2.5.0",
    "@hapi/good-squeeze": "^6.0.0",
    "@hapi/hapi": "^20.0.1",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "boom": "^7.3.0",
    "chalk": "^4.1.0",
    "hapi-auth-jwt2": "^10.2.0",
    "hoek": "^6.1.3",
    "joi": "^17.3.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.10.15",
    "querystring": "^0.2.0",
    "stream": "0.0.2",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.33",
    "@types/boom": "^7.3.0",
    "@types/hapi__hapi": "^20.0.1",
    "@types/hapi__inert": "^5.2.2",
    "@types/hapi__vision": "^5.5.2",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.9",
    "hapi-swagger": "^14.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  }
}
